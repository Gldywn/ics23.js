{
  "name": "@gldywn/ics23.js",
  "version": "1.0.0",
  "description": "Provides JavaScript/TypeScript bindings for the official Rust ics23 crate, compiled to WebAssembly via wasm-bindgen and wasm-pack.",
  "homepage": "https://github.com/Gldywn/ics23.js#readme",
  "bugs": {
    "url": "https://github.com/Gldywn/ics23.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gldywn/ics23.js.git"
  },
  "type": "module",
  "license": "Apache-2.0",
  "author": "Gldywn",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "rust/pkg",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22.16.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "jest --runInBand",
    "test:coverage": "npm run test -- --coverage",
    "build": "npm run build:wasm && npm run build:ts",
    "build:ts": "tsup",
    "build:wasm": "wasm-pack build rust --release --target nodejs"
  },
  "keywords": [
    "ics23",
    "ics-23",
    "merkle",
    "merkle-proof",
    "verification",
    "membership",
    "non-membership",
    "batch-proof",
    "webassembly",
    "wasm",
    "typescript",
    "nodejs",
    "cosmos",
    "ibc",
    "tendermint",
    "iavl",
    "sparse-merkle-tree",
    "smt",
    "light-client",
    "cryptography"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.1.0",
    "typescript": "^5.9.2",
    "wasm-pack": "^0.13.1"
  },
  "dependencies": {
    "@cosmjs/encoding": "^0.36.0"
  }
}